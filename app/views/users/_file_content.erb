
  <div id="content">
    <div><%= link_to(image_tag('scrollup.png'), id: 'scrollup')%></div>
    <div class="clear"></div>
    <div class="media">
      <% current_user.resources.each do |resource|%>
      <div class="media-left">
        <!-- Renders image -->
        <%= link_to image_tag(resource.file.url, class: 'media-object'), resource.file.url, target: '_blank' %>
      </div>
        <div class="media-body">
          <h4 class="media-heading"><%= photo.name %></h4>
        </div>
      <% end %>
    </div>
  </div>
